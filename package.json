{
  "name": "book_a_meal",
  "version": "1.0.0",
  "description": "Book-A-Meal is an application that allows customers to make food orders and helps the food vendor know what the customers want to eat.",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --exec babel-node api/index.js",
    "test": "mocha ./api/tests/**.spec.js --recursive --require @babel/register --timeout 10000 --exit",
    "test-watch": "nodemon --exec \"npm test\"",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Omobolanle-Arogundade/book_a_meal.git"
  },
  "keywords": [
    "Book-a-Meal",
    "Andela"
  ],
  "author": "Bolanle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Omobolanle-Arogundade/book_a_meal/issues"
  },
  "homepage": "https://github.com/Omobolanle-Arogundade/book_a_meal#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "expect": "^1.20.2",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.3",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "pg": "^7.8.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.43.0",
    "sequelize-cli": "^5.4.0",
    "trim-request": "^1.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "supertest": "^3.4.2"
  }
}
